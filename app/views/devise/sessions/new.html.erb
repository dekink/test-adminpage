<html >
  <head>
    <meta charset="UTF-8">
    <title>Random Login Form</title>
    
    <link href="assets/style.css" rel="stylesheet">
    <script src="assets/prefixfree.min.js"></script>
  </head>

  <body>

    <div class="body"></div>
		<div class="grad2"></div>
		<div class="header">
			<div>LIKE LIKE LIKE LIKE LIKE LIKE LIKE LIKE LIKE <br>
			     </div>
		</div>
		<br>
		<div class="login">
		  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
				<%= f.email_field :email, autofocus: true %><br>
				<%= f.password_field :password, autocomplete: "off" %><br>
				<button class="btn btn-warning"><% f.submit "Log in" %>LOGIN</button>
				<br>
				<%= render "devise/shared/links" %>
			<% end %>
		</div>
    <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>
  </body>
</html>
